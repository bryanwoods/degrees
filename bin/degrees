  args = process.argv;
  zip = args[2] ? args[2] : "11231";
  jsdom.env(BASE_URL + zip, ['http://code.jquery.com/jquery-1.5.min.js'], function(errors, window) {
    var $, high, low;
    $ = window.jQuery;
    high = $('.fctHiLow .b').html();
    low = $('.fctHiLow').first().html().split('|')[1].split('Â°')[0].trim();
    console.log("Today's high: " + high);
    return console.log("Today's low: " + low);
  });
}).call(this);
